  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.4.2",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.17",
